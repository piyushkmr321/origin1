package com.myabstract;

public class Circle extends Shape {
    private int radius;
    
    public Circle() {
    	radius=10;
    }
    
    public Circle(int radius)
    {
    	super();
    	return 3.14*radius*radius;
    }
    
    
    
    
	@Override
	public double area() {
		// TODO Auto-generated method stub
		return 0;
	}

}
