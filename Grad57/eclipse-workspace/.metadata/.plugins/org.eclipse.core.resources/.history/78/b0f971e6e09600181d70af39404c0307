
public class Demo1D {
	public static void main(String[] args) {
		int [] numbers;
		numbers = new int[5];
		numbers[0]=10;
		numbers[1]=200;
		numbers[2]=10000;
		numbers[3]=222;
		numbers[4]=5000;
		int c=numbers[0]+numbers[1]+numbers[2]+numbers[3]+numbers[4];
		
		System.out.println("sum: "+c);
		int i;
		for(i=0;i<3;i++)
		{
			System.out.println(numbers[i]);
		}
	}

}
