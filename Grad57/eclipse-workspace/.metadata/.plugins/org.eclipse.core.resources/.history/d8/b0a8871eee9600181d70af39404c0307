
public class Demo1D {
	public static void main(String[] args) {
		int [] numbers;
		numbers = new int[5];
		numbers[0]=10;
		numbers[1]=200;
		numbers[2]=10000;
		numbers[3]=222;
		numbers[4]=5000;
		int c=numbers[0]+numbers[1]+numbers[2]+numbers[3]+numbers[4];
		
		System.out.println("sum: "+c);
		for(int i=0;i<3;i++)
		{
			System.out.println(numbers[i]);
		}
		for(int x:numbers)
		{
			System.out.println(x);
		}
		int [] arr;
		arr=new int[11];
		arr[]= {12,34,44,54,34,55,65,78,90,23,90};
		int i=0;
		while(i<11)
		{
			System.out.println(arr[i]);
i++;		}
		
		
		
	}

}
