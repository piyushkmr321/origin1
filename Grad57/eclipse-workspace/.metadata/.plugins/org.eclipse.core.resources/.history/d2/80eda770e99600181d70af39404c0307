
public class Demo2D {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int [][] arrays;
		arrays=new int[3][3];
		arrays[0][0]=1;
		arrays[0][1]=2;
		arrays[0][2]=3;
		
		arrays[1][1]=4;
		arrays[1][2]=5;
		arrays[1][3]=6;
		
		arrays[2][1]=7;
		arrays[2][2]=8;
		arrays[2][3]=9;
	
	for(int i=0;i<3;i++)
	{
		for (int j=0;j<3;j++) {
		System.out.print(arrays+"\t");}
	}
		System.out.println();
	
		
		
		
		
		
		
		
		
	}

}
