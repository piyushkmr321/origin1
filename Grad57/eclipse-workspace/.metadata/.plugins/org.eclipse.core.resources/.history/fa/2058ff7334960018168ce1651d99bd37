
public class person {

	private int age;
	private String name;
	
	public void display() {
		System.out.println("name:"+name +"\t"+"age:"+age);
	}
	
	public person()
	{
		age=22;
		name="piyush";
	}
	public person(int a, String n)
	{
		age=a;
		name=n;
	}
	
	public void setAge(int a)
	{
		age=a;
	}
}
