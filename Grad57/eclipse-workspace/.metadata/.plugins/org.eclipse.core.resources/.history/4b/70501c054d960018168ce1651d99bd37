
public class person {

	private int age;
	private String name;
	
	public void display() {
		System.out.println("name:"+name +"\t"+"age:"+age);
	}
	
	public person()
	{
		age=22;
		name="piyush";
	}
	public person(int age, String name)
	{
		this.age=age;
		this.name=name;
	}
	
	public void setAge(int a)
	{
		age=a;
	}
}
